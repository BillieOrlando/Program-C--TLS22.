{
    float rumus, volumeBalok, p, l, t;
    int volumebalok ;
    
    cout<<"***** Volume Balok ******";
    cout<<endl;
    if(volumeBalok == 1)
    cout<<"Volume Balok \n" ;
    cout<<"\nmasukkan p :";
